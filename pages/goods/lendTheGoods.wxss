input.invite-code {
  border: solid 1px rgb(191,191,191);
  padding: 30rpx;
  width: 622rpx;
  margin: 0 auto;
}
input::-webkit-input-placeholder {
  color: rgb(153,153,153);
}
.confirm-btn {
  width:295rpx;
  height:85rpx;
  color: #fff;
  border-radius: 40rpx;
  border: none;
  background: rgb(114,207,221);
  font-size: 32rpx;
  margin: 0;
}
.confirm-btn:first-child {
  margin-right: 22rpx;
}
.button-hover {
  opacity: .8;
}
.btn-box {
  padding-bottom: 60rpx;
  padding-top: 23rpx;
}
.btn-box image{
  width: 296rpx;
  height: 106rpx;
}
.graybtn{
  background: #999999!important;
  border: 1px solid #999999!important;
}
.nheader{
  padding: 25rpx 30rpx;
  padding-bottom: 22rpx;
  background: #fff;
  margin-top:20rpx;
}
.nheader .icons{
  width: 36rpx;
  height: 36rpx;
  margin-right: 20rpx;
}

.buyerinfo{
  flex: 1;
}
.buyerinfo text{
  display: block;
  margin-bottom: 13rpx;
  color: #464646;
  font-size: 30rpx;

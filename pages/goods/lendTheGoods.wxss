page {
  background: #f5f5f5;
  height:100%;
}
.icon_group_buying {
  width: 32rpx;
  height: 32rpx;
  margin-right: 20rpx;
  margin-top: 8rpx;
}
.group_details_box {
  padding: 40rpx 32rpx;
  line-height: 50rpx;
  background: #fff;
}
.group_details_box text {
  font-size: 32rpx;
}
.icon_check_isok {
  width: 202rpx;
  height: 46rpx;
}
.icon_check_yes {
  width: 48rpx;
  height: 48rpx;
  margin-right: 16rpx;
}
.checkOk {
  height: 164rpx;
}
.shop-lists .main-title {
  font-size:32rpx;
  height:91rpx;
  line-height:91rpx;
  padding:0 30rpx;
  border-bottom:1px solid #eee;
}
.lists-box {
  padding: 5rpx 30rpx;
}
.list-item {
  position: relative;
  padding:22rpx 0;
}
.checkedimg,.ncheckedimg{
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  top: 52rpx;
  left: 39rpx;
}
.lingqu{
    width: 129rpx;
  height: 108rpx;
  position: absolute;
  top: 0rpx;
  left:0rpx;
  z-index: 100;
}
.list-item .shop-img {
 width: 95rpx;
 height: 95rpx;
 margin-right: 22rpx;
}
.list-item .con{
  flex: 1;
}
.list-item .con .shop-title{
color: #333333;
font-size: 29rpx;
 text-overflow:clip;
  letter-spacing:2rpx;
  overflow:hidden;
  white-space:nowrap;
  font-family:UICTFontTextStyleBody;
  margin-bottom: 13rpx;
}
.list-item .con .total-count{
justify-content:space-between;
}

.list-item .con .total-count text{
color: #636363;
font-size: 24rpx;
}

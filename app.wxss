/*进度条*/
.progress{
  width:328rpx;
  height:12rpx;
  background:#FFCF3B;
  position:relative;
  border-radius:100rpx;
  margin-top:10rpx;
}
.progress_bar{
  position:absolute;
  left:0;
  background:#FF9000;
  width:20%;
  height:100%;
  border-radius:100rpx;
}
.progress_bar view{
  width:8rpx;
  height:8rpx;
  border-radius:100%;
  position:absolute;
  right:2rpx;
  top:2rpx;
}
/*超过两行显示省略号*/
.ellipsis_2{
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.mask{
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: rgba(0,0,0,0.4);
  z-index: 999;
}
